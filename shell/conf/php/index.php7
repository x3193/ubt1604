function hanpc2enpc($str){
        $str = str_replace("，",",",$str);       
        $str = str_replace("。",".",$str);       
        $str = str_replace("！","!",$str);       
        $str = str_replace("？","?",$str);       
        $str = str_replace("：",":",$str);       
        $str = str_replace("；",";",$str);       
        $str = str_replace("——","-",$str);       
        $str = str_replace("（","(",$str);       
        $str = str_replace("）",")",$str);       
        $str = str_replace("“","\"",$str);       
        $str = str_replace("”","\"",$str);       
        $str = str_replace("《","<",$str);       
        $str = str_replace("》",">",$str);       
        $str = str_replace("‘","\'",$str);       
        $str = str_replace("’","\'",$str);       
        $str = str_replace("……","...",$str);       
        $str = str_replace("￥","$",$str);  
        return $str;
}

